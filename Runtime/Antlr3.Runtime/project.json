{
  "version": "3.6.0-*",
  "title": "ANTLR 3 Runtime",
  "description": "The runtime library for parsers generated by the C# target of ANTLR 3. This package supports projects targeting .NET 2.0 or newer, and built using Visual Studio 2008 or newer.",
  "authors": [ "Sam Harwell", "Terence Parr" ],
  "copyright": "Copyright © Sam Harwell 2013",

  "packOptions": {
    "owners": [ "Sam Harwell" ],
    "projectUrl": "https://github.com/antlr/antlrcs",
    "releaseNotes": "https://github.com/antlr/antlrcs/releases/$version$",
    "iconUrl": "https://raw.github.com/antlr/website-antlr4/master/images/icons/antlr.png",
    "licenseUrl": "https://raw.githubusercontent.com/antlr/antlrcs/master/build/prep/LICENSE.txt",
    "requireLicenseAcceptance": true,
    "summary": "The runtime library for parsers generated by the C# target of ANTLR 3.",
    "tags": [ "antlr", "antlr3", "parsing" ]
  },

  "buildOptions": {
    "xmlDoc": "true",
    "keyFile": "../../../../../keys/antlr/Key.snk"
  },

  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "TRACE" ],
        "optimize": true
      }
    }
  },

  "frameworks": {
    "net20": {},
    ".NETFramework,Version=v4.0,Profile=Client": {},
    ".NETPortable,Version=v4.0,Profile=Profile328": {
      "buildOptions": {
        "define": [ "PORTABLE", "NET40", "NET40PLUS", "NET35PLUS", "NET20PLUS" ]
      },
      "dependencies": {},
      "frameworkAssemblies": {
        "mscorlib": "",
        "System": "",
        "System.Core": ""
      }
    },
    ".NETPortable,Version=v4.5,Profile=Profile259": {
      "buildOptions": {
        "define": [ "PORTABLE", "NET45", "NET45PLUS", "NET40PLUS", "NET35PLUS", "NET20PLUS" ]
      },
      "dependencies": {},
      "frameworkAssemblies": {
        "Microsoft.CSharp": "",
        "mscorlib": "",
        "System.Collections": "",
        "System.Diagnostics.Debug": "",
        "System.IO": "",
        "System.Reflection": "",
        "System.Reflection.Extensions": "",
        "System.Runtime": "",
        "System.Runtime.Extensions": "",
        "System.Runtime.Serialization.Primitives": "",
        "System.Text.Encoding": "",
        "System.Text.RegularExpressions": ""
      }
    },
    "netstandard1.1": {
      "buildOptions": {
        "define": [ "NET_STANDARD", "NO_BINARY_SERIALIZATION", "NO_SYSTEM_CONSOLE", "NO_REGEXOPTIONS_COMPILED", "LIMITED_OR_NO_STACKTRACE", "NO_SYSTEM_IO_FILE", "NO_SYSTEM_SECURITY_PERMISSIONS" ]
      },
      "dependencies": {
        "System.Collections": "4.0.11",
        "System.Diagnostics.Debug": "4.0.11",
        "System.Diagnostics.Tracing": "4.1.0",
        "System.Runtime.Extensions": "4.1.0",
        "System.Runtime.InteropServices": "4.1.0",
        "System.Runtime.Serialization.Primitives": "4.1.1",
        "System.Text.RegularExpressions": "4.1.0"
      }
    },
    "netstandard1.3": {
      "buildOptions": {
        "define": [ "NET_STANDARD", "NO_BINARY_SERIALIZATION", "LIMITED_OR_NO_STACKTRACE", "NO_SYSTEM_SECURITY_PERMISSIONS" ]
      },
      "dependencies": {
        "System.Collections": "4.0.11",
        "System.Console": "4.0.0",
        "System.Diagnostics.Debug": "4.0.11",
        "System.Diagnostics.StackTrace": "4.0.2",
        "System.Diagnostics.Tracing": "4.1.0",
        "System.IO.FileSystem": "4.0.1",
        "System.Runtime.Extensions": "4.1.0",
        "System.Runtime.InteropServices": "4.1.0",
        "System.Runtime.Serialization.Primitives": "4.1.1",
        "System.Text.RegularExpressions": "4.1.0"
      }
    }
  }
}